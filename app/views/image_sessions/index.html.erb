<p id="notice"><%= notice %></p>

<h1>Image Sessions. Overall list</h1>

<table>
  <thead>
    <tr>
      <th>Greenright</th>
      <th>Blueright</th>
      <th>Greenwrong</th>
      <th>Bluewrong</th>
      <th>Colorlesswrong</th>
      <th>Greenleft</th>
      <th>Blueleft</th>
      <th>Image</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% current_user.image_sessions.each do |image_session| %>
      <tr>
        <td><%= image_session.greenRight %></td>
        <td><%= image_session.blueRight %></td>
        <td><%= image_session.blueWrong %></td>
        <td><%= image_session.colorlessWrong %></td>
        <td><%= image_session.greenLeft %></td>
        <td><%= image_session.blueLeft %></td>
        <td><%= image_session.image_id %></td>
        <td><%= image_session.user_id %></td>
        <td><%= link_to 'Show', image_session %></td>
        <td><%= link_to 'Edit', edit_image_session_path(image_session) %></td>
        <td><%= link_to 'Destroy', image_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

